{
  "manifest_version": 2,
  "name": "Cryptopup finder",
  "description": "foo",
  "version": "1.0",
  "browser_action": {
    "default_title": "Cryptopup"
  },
  "content_scripts": [
    {
      "matches": ["https://tron.game.com/*"],
      "js": ["scripts.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs", "https://tron.game.com/*"
  ]
}
